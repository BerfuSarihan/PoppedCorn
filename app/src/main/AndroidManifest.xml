<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/_90630"
        android:label="ABSMovie"
        android:supportsRtl="true"
        android:theme="@style/Theme.PoppedCorn"
        android:usesCleartextTraffic="true"
        tools:ignore="ExtraText"
        tools:targetApi="31">
        <activity
            android:name=".AddMovieActivity"
            android:label="Add Movie"
            android:exported="false" />
        <activity
            android:name=".ViewMovieActivity"
            android:label="View Movie"
            android:exported="false" />
        <activity
            android:name=".ContactActivity"
            android:label="Contact"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MovieListActivity"
            android:label="Movie List" />
    </application>

</manifest>